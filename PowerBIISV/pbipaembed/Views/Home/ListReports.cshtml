@model pbipaembed.Models.ReportsViewModel

@{
    ViewBag.Title = "List Reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Select a report:</h2>
@using (Html.BeginForm("EmbedReport", "Home", FormMethod.Post))
{
    @Html.DropDownList("SelectedReport", new SelectList(Model.Reports, "Id", "Name"))
    <input type="submit" value="Embed" />
}